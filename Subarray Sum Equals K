class Solution:
    def subarraySum(self, nums: List[int], k: int) -> int:
        count = Counter()
        count[0]+=1
        curr_sum = 0
        result = 0
        for i in nums:
            curr_sum+=i
            if curr_sum-k in count:
                result+=count[curr_sum-k]
            count[curr_sum]+=1
        return result
